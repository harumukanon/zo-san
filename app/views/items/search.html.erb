			  <% if @items.any? %>
          <table class="table table-hover">
            <thead>
              <tr>
                <th class="col-xs-1">資料id</th>
                <th class="col-xs-3">書名</th>
                <th class="col-xs-2">著者名</th>
                <th class="col-xs-2">刊年</th>
                <th class="col-xs-2">出版者</th>
                <th class="col-xs-2">所蔵</th>
              </tr>
            </thead>
            <tbody>
              <% @items.each do |f| %>
              <tr>
                <td><%= link_to f.id, item_path(f) %></td>
                <td><%= f.title %></td>
                <td><%= f.author %></td>
                <td><%= f.year %></td>
                <td><%= f.publisher %></td>
                <td><%= f.holding %></td>
                <td id="item-<%= f.id %>">
                  <% if @stamp.stamped?(f) %>
                  <%= render partial: 'not_stamped_at', locals: {item: f} %>
                  <% else %>
                  <%= render partial: 'stamped_at', locals: {item: f} %>
                  <% end %>
                </td>
              <% end %>
            </tbody>
          </table>
        <%= link_to "資料を新規登録", new_item_path(stamp_id: @stamp.id) %>
          <% if @stamp.present? %>
            <%= link_to "蔵書印情報へ戻る", stamp_path(@stamp) %>
          <% end %>
        <% end %>