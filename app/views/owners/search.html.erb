			  <% if @owners.any? %>
          <table class="table table-hover">
            <thead>
              <tr>
                <th class="col-xs-1">蔵書印主id</th>
                <th class="col-xs-5">名称</th>
                <th class="col-xs-2">別名1</th>
                <th class="col-xs-2">別名2</th>
                <th class="col-xs-2">別名3</th>
              </tr>
            </thead>
            <tbody>
              <% @owners.each do |f| %>
              <tr>
                <td><%= f.id %></td>
                <td><%= f.aap %></td>
                <td><%= f.vap1 %></td>
                <td><%= f.vap2 %></td>
                <td><%= f.vap3 %></td>
                <td id="owner-<%= f.id %>">
                  <% if @stamp.own?(f) %>
                  <%= render partial: 'not_owned', locals: {owner: f} %>
                  <% else %>
                  <%= render partial: 'owned', locals: {owner: f} %>
                  <% end %>
                </td>
              <% end %>
            </tbody>
          </table>
        <% end %>